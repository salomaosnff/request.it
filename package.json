{
  "name": "request-it",
  "displayName": "request.it",
  "description": "HTTP client",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.55.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:request-it.create-request",
    "onView:request-it.requests"
  ],
  "main": "./dist/extension.js",
  "extensionKind": [
    "ui"
  ],
  "contributes": {
    "commands": [
      {
        "title": "Refresh",
        "command": "request-it.refresh",
        "icon": {
          "dark": "resources/dark/refresh.svg",
          "light": "resources/light/refresh.svg"
        }
      },
      {
        "title": "New Request",
        "command": "request-it.create-request",
        "icon": {
          "dark": "resources/dark/add.svg",
          "light": "resources/light/add.svg"
        }
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "request-it",
          "title": "Request It",
          "icon": "resources/icon.png"
        }
      ]
    },
    "views": {
      "request-it": [
        {
          "id": "request-it.requests",
          "type": "tree",
          "name": "Requests"
        },
        {
          "id": "request-it.history",
          "type": "tree",
          "name": "History"
        },
        {
          "id": "request-it.enviroment",
          "type": "tree",
          "name": "Enviroment"
        }
      ]
    },
    "menus": {
      "view/title": [
        {
          "when": "view == request-it.requests",
          "command": "request-it.refresh",
          "group": "navigation"
        },
        {
          "when": "view == request-it.requests",
          "command": "request-it.create-request",
          "group": "navigation"
        }
      ]
    },
    "customEditors": [
      {
        "viewType": "request-it.editor",
        "displayName": "Request.it Editor",
        "selector": [
          {
            "filenamePattern": ".request-it/requests/rooms/browser.json"
          }
        ],
        "priority": "default"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "test-compile": "tsc -p ./",
    "test-watch": "tsc -watch -p ./",
    "pretest": "yarn run test-compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/lodash.pick": "^4.4.6",
    "@types/mocha": "^8.0.4",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.55.0",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "eslint": "^7.19.0",
    "glob": "^7.1.6",
    "mocha": "^8.2.1",
    "ts-loader": "^8.0.14",
    "typescript": "^4.1.3",
    "vscode-test": "^1.5.0",
    "webpack": "^5.19.0",
    "webpack-cli": "^4.4.0"
  },
  "dependencies": {
    "lodash.pick": "^4.4.0"
  }
}
